    "@babel/register": "^7.0.0",
    "@crowdin/cli": "^3.5.2",
    "@jest/globals": "workspace:*",
    "@jest/test-utils": "workspace:*",
    "@types/babel__core": "^7.0.0",
    "@types/babel__generator": "^7.0.0",
    "@types/babel__template": "^7.0.0",
    "@types/dedent": "0.7.0",
