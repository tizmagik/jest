{
  "jest": {
    "transform": {
      "\\.css$": "<rootDir>/cssPreprocessor.js",
      "\\.js$": "<rootDir>/jsPreprocessor.js",
      "\\.svg$": "<rootDir>/filePreprocessor.js"
    },
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "jest-circus",
      "jest-environment-node",
      "jest-jasmine2",
      "jest-runner"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "react": "*",
    "react-dom": "*",
    "react-test-renderer": "*"
  }
}
